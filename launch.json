{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "TST_extract",
            "type": "debugpy",
            "program": "src/main.py",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "--load_model", "outputs/regression_embeddings_2024-05-09_14-14-48_x37/checkpoints/model_best.pth",
                "--test_only", "testset",
                "--extract_embeddings_only", "raw",
                "--test_pattern", "regression/FloodModeling1/FloodModeling1_TEST.ts",
                "--output_dir", "embeddings", 
                "--comment", "regression from scratch",
                "--name", "regression_embeddings",
                "--records_file", "regression_records_embeddings.xls",
                "--data_dir", "regression/FloodModeling1",
                "--data_class", "tsra",
                "--pattern", "TEST",
                "--val_ratio", "0.0",
                "--epochs", "10",
                "--lr", "0.001",
                "--optimizer","RAdam",
                "--pos_encoding","learnable",
                "--task","imputation",
                "--d_model", "128",
            ]
        },
        {
            "name": "TST_pretrain",
            "type": "debugpy",
            "program": "src/main.py",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "--output_dir", "outputs", 
                "--comment", "test fnirs training on high density dataset",
                "--name", "fnirs_high_density_imputation",
                "--records_file", "fnirs_high_density_test.xls",
                "--data_dir", "imputation",
                "--data_class", "fnirs",
                "--pattern", "mini",
                "--val_ratio", "0.2",
                "--test_ratio", "0.1",
                "--epochs", "500",
                "--lr", "0.001",
                "--lr_step", "1000000",
                "--lr_factor", "0.1",
                "--optimizer","RAdam",
                "--pos_encoding","learnable",
                "--task","imputation",
                "--d_model", "1024",
                "--dim_feedforward", "2028",
                "--num_heads", "8",
                "--batch_size", "64",
                "--dropout", "0.1",
                "--activation", "gelu",
                "--no_timestamp",
                "--subsample_factor", "1",
                "--model", "transformer",
                "--normalization_layer", "LayerNorm",
                "--max_seq_len", "500",
                "--gpu", "-1",
            ] 
        }
    ]
}